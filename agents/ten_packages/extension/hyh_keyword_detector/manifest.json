{
  "type": "extension",
  "name": "hyh_keyword_detector",
  "version": "0.1.0",
  "language": "python",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.2"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md"
    ]
  },
  "api": {
    "property": {},
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          },
          "stream_id": {
            "type": "uint32"
          },
          "end_of_segment": {
            "type": "bool"
          }
        }
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      },
      {
        "name": "pause_realtime_v2v"
      },
      {
        "name": "start_realtime_v2v"
      }
    ]
  }
}